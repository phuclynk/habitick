{
  "name": "habitick",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite --port=3000",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "test": "vitest",
    "postinstall": "husky install",
    "cy:open": "cypress open",
    "cy:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cy:open",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cy:headless",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "types:check": "tsc --project tsconfig.json --pretty --noEmit",
    "generate": "plop",
    "storybook:open": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@hookform/resolvers": "^2.9.10",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-query-devtools": "^4.20.4",
    "axios": "^1.2.2",
    "clsx": "^1.2.1",
    "dayjs": "^1.10.6",
    "dompurify": "^2.4.3",
    "history": "^5.3.0",
    "intersection-observer": "^0.12.0",
    "marked": "^2.0.7",
    "nanoid": "^3.1.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.3",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.41.3",
    "react-query-auth": "^2.0.0",
    "react-router-dom": "^6.6.1",
    "type-fest": "^1.2.0",
    "typescript": "^4.1.2",
    "zod": "^3.20.2",
    "zustand": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@mswjs/data": "^0.3.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^2.2.2",
    "@types/faker": "^5.5.7",
    "@types/jest": "^29.2.5",
    "@types/marked": "^2.0.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/ui": "^0.26.3",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "faker": "^5.5.3",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "lint-staged": "^15.2.0",
    "msw": "^0.29.0",
    "plop": "^2.7.4",
    "postcss": "^8.4.20",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "start-server-and-test": "^1.12.5",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.3",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
